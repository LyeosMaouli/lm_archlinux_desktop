# CPU frequency scaling configuration
# Generated by Ansible for {{ ansible_hostname }}

# Default governor for CPU frequency scaling
GOVERNOR="{{ cpu_governor_default | default('powersave') }}"

# Minimum frequency
MIN_SPEED="{{ cpu_min_freq | default('800000') }}"

# Maximum frequency  
MAX_SPEED="{{ cpu_max_freq | default('0') }}"

# Enable CPU frequency scaling
ENABLE="true"

# Additional options
OPTIONS=""